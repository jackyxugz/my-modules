<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data noupdate="1">
         <record id="lxb_core_course_sequence_code" model="ir.sequence">
              <field name="name">course_code</field>
              <field name="code">lxb.course</field>
              <field name="prefix">CUR%(y)s%(month)s</field>
              <field name="padding">5</field>
              <field name="company_id" eval="False"/>
         </record>
         <record id="lxb_core_schedule_sequence_code" model="ir.sequence">
              <field name="name">schedule_code</field>
              <field name="code">lxb.schedule</field>
              <field name="prefix">SCH%(y)s%(month)s</field>
              <field name="padding">4</field>
              <field name="company_id" eval="False"/>
         </record>
         <record id="lxb_core_class_sequence_code" model="ir.sequence">
              <field name="name">class_code</field>
              <field name="code">lxb.class</field>
              <field name="prefix">CLASS%(y)s%(month)s</field>
              <field name="padding">3</field>
              <field name="company_id" eval="False"/>
         </record>
     </data>
</odoo>